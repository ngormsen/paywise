        row="0"
        rowSpan="5"
        col="0"
        stretch="aspectFill"
        src="~/images/fifth_coachmark.png"
        id="userIcon" />

    
    <StackLayout row="1" class="btngotit">
         <Button tap="onOrdersTap" class="buttonprimary" text="Okay"/>
    </StackLayout>
</GridLayout>

</page>

